{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "talcs",
  "description": "TALCS: Audio Library of CrSjimo",
  "version": "0.0.1.1",
  "homepage": "https://talcs.sjimo.dev/",
  "license": "LGPL-2.1",
  "dependencies": [
    "libsndfile",
    "rpclib",
    "rtmidi",
    {
      "name": "sdl2",
      "default-features": false
    },
    "interval-tree",
    "boost-interprocess",
    "qmsetup",
    "wavpack",
    "r8brain-free-src",
    "portaudio",
    "libsoundio"
  ],
  "overrides": [
    {
      "name": "sdl2",
      "version": "2.28.2"
    }
  ],
  "builtin-baseline": "dd6fbdd22cc56af2ec8f97b4f62ce5ef612a891b",
  "vcpkg-configuration": {
    "overlay-ports": [
      "../vcpkg/ports"
    ],
    "overlay-triplets": [
      "../vcpkg/triplets"
    ]
  }
}
